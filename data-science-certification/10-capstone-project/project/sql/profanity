update profanity set word = lower(word);
update profanity set word = trim(word);

select * from unigram_final where word in (select word from profanity);
delete from unigram_final where word in (select word from profanity);

select * from bigram_final where word in (select word from profanity);
delete from bigram_final where word in (select word from profanity);

select * from trigram_final where word in (select word from profanity);
delete from trigram_final where word in (select word from profanity);

select * from fourgram_final where word in (select word from profanity);
delete from fourgram_final where word in (select word from profanity);
